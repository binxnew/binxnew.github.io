<div class="container-fluid fellowship-container justify-content-center" style="	background-color: #ecf0f1;
">

	<div class="container" style="width: 90%; padding: 0px 0 40px 0">

		<div class="fellow-detail-breadcrumb">
		  <a href="./fellowship_fellows.html"><i class="fas fa-chevron-left"></i>&nbsp;Back</a>
		</div>

		<div>
			<div style="font-size: 25px; font-weight: 700">{{project_name}}</div> 
			<div style="font-size: 18px;">by {{fellow_names}}</div>
		</div>

		{{#if youtube_url}}

		<div class="justify-content-center" style="text-align: center; padding-top: 20px">

			<div class="justify-content-center" style="text-align: center; padding: 0 40px">
				<style>.embed-container { position: relative; padding-bottom: 56.25%; height: 0; overflow: hidden; max-width: 100%; } .embed-container iframe, .embed-container object, .embed-container embed { position: absolute; top: 0; left: 0; width: 100%; height: 100%; }</style><div class='embed-container'><iframe src='{{youtube_url}}' frameborder='0' allowfullscreen></iframe></div>
			</div>
		</div>

  		{{/if}}

		<div style="margin-top: 30px; font-weight: bold; font-size: 18px">
			Project Description
		</div>
		<div style="margin-top: 5px;">
			{{{project_description}}}
		</div>
		<div style="margin: 20px 0 10px 0; font-weight: bold; font-size: 18px">
			Fellows
		</div>
		{{#each fellows}}
		  <div style="margin-top:5px; font-weight: bold; font-style: italic">{{fellow_name}}</div>
		  <div style="margin-top:5px">{{fellow_bio}}</div>
		  <div class='b-para'>
			Follow {{fellow_name}} at: <a href="{{social_media_url}}" target="_new">{{social_media_handle}}</a>
		  </div>
		{{/each}}

	</div>

</div>
